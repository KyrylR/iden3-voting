{
  "name": "vue-vite-template",
  "version": "1.0.0-rc.1",
  "gitHooks": {
    "pre-commit": "yarn lint",
    "pre-push": "yarn rsc"
  },
  "scripts": {
    "vite-dev-server": "vite",
    "vite-build": "vite build",
    "start": "dotenv -e .env.local yarn vite-dev-server",
    "build": "dotenv -e .env.production yarn vite-build",
    "analyze": "dotenv -e .env.analyze yarn vite-build",
    "lint-fix": "prettier .  --write",
    "lint": "yarn lint:styles",
    "lint:styles": "stylelint \"src/**/*.{vue,scss,css}\" --max-warnings=0",
    "preview": "vite preview",
    "rsc": "node scripts/release-sanity-check.mjs"
  },
  "dependencies": {
    "@distributedlab/tools": "^1.0.0-rc.16",
    "@iden3/js-crypto": "^1.1.0",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.10.0",
    "date-fns": "^3.6.0",
    "ethers": "^6.12.2",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.1",
    "pinia": "^2.1.7",
    "postcss": "^8.4.38",
    "snarkjs": "^0.7.4",
    "timeago.js": "^4.0.2",
    "uuid": "^9.0.1",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20.13.0",
    "@types/uuid": "^9.0.8",
    "@types/vuelidate": "^0.7.21",
    "@vitejs/plugin-vue": "^5.0.5",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.4",
    "stylelint": "^16.6.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-declaration-strict-value": "^1.10.4",
    "stylelint-scss": "^6.3.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "yorkie": "^2.0.0"
  }
}
